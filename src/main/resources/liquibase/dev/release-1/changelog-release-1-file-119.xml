				onDelete="RESTRICT"
				onUpdate="RESTRICT"
				referencedTableSchemaName="apps"
				referencedTableName="mdfem_undercount_calc_method"
				referencedColumnNames="code"
		/>
	</changeSet>


</databaseChangeLog>
