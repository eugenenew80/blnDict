<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">    

    <changeSet id="dict-eug-106" logicalFilePath="path-independent" author="eugene" context="postgres">
        <comment>Creating table: dict_metering_point_current_trans</comment>
        <createTable tableName="dict_metering_point_current_trans" schemaName="apps">            
            <column name="id" type="number">
                <constraints nullable="false"/>
            </column>

            <column name="metering_point_id" type="number">
                <constraints nullable="false"/>
            </column>

            <column name="current_trans_id" type="number">
                <constraints nullable="false"/>
            </column>
                        
            <column name="start_date" type="date">
                <constraints nullable="true"/>
            </column>
                                    			                                                 
            <column name="end_date" type="date">
                <constraints nullable="true"/>
            </column>              
        </createTable>
    </changeSet>

	<changeSet id="dict-eug-107" logicalFilePath="path-independent" author="eugene" context="postgres">
	    <comment>Creating primary key for table: dict_metering_point_current_trans</comment>
	    <addPrimaryKey 
	            columnNames="id"
	            constraintName="pk_dict_metering_point_current_trans"
	            schemaName="apps"
	            tableName="dict_metering_point_current_trans"/>
	</changeSet>
	
	<changeSet id="dict-eug-108" logicalFilePath="path-independent" author="eugene" context="postgres">
	    <comment>Creating sequence for table: dict_metering_point_current_trans</comment>
	    <createSequence 
	            incrementBy="1"
	            schemaName="apps"
	            sequenceName="dict_metering_point_current_trans_s"
	            startValue="1"/>
	</changeSet>	
</databaseChangeLog>
