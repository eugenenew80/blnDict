<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <changeSet id="dict-eug-65" logicalFilePath="path-independent" author="eugene" context="dev">
        <comment>Creating table: dict_energy_source_business_partners</comment>
        <createTable tableName="dict_energy_source_business_partners" schemaName="apps">            
            <column name="id" type="number">
                <constraints nullable="false"/>
            </column>
                        
            <column name="energy_source_id" type="number">
                <constraints nullable="false"/>
            </column>

            <column name="business_partner_id" type="number">
                <constraints nullable="false"/>
            </column>
            
            <column name="start_date" type="date">
                <constraints nullable="true"/>
            </column>
                                    			                                                 
            <column name="end_date" type="date">
                <constraints nullable="true"/>
            </column>              
        </createTable>
    </changeSet>

	<changeSet id="dict-eug-66" logicalFilePath="path-independent" author="eugene" context="dev">
	    <comment>Creating primary key for table: dict_energy_source_business_partners</comment>
	    <addPrimaryKey 
	            columnNames="id"
	            constraintName="pk_dict_energy_source_business_partners"
	            schemaName="apps"
	            tableName="dict_energy_source_business_partners"/>
	</changeSet>
	
	<changeSet id="dict-eug-67" logicalFilePath="path-independent" author="eugene" context="dev">
	    <comment>Creating sequence for table: dict_energy_source_business_partners</comment>
	    <createSequence 
	            incrementBy="1"
	            schemaName="apps"
	            sequenceName="dict_energy_source_business_partners_s"
	            startValue="1"/>
	</changeSet>	
</databaseChangeLog>
