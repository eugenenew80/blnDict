<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

  <mapping map-null="false" map-empty-string="false" trim-strings="true">
    <class-a>kz.kegoc.bln.entity.dict.PowerLinePart</class-a>
    <class-b>kz.kegoc.bln.entity.dict.dto.PowerLinePartDto</class-b>

    <field>
      <a>powerLine.id</a>
      <b>powerLineId</b>
    </field>

    <field type="one-way">
      <a>powerLine.name</a>
      <b>powerLineName</b>
    </field>

    <field>
      <a>businessPartner.id</a>
      <b>businessPartnerId</b>
    </field>

    <field type="one-way">
      <a>businessPartner.shortName</a>
      <b>businessPartnerShortName</b>
    </field>

    <field>
      <a>org.id</a>
      <b>orgId</b>
    </field>

    <field type="one-way">
      <a>org.shortName</a>
      <b>orgShortName</b>
    </field>
  </mapping>
</mappings>
