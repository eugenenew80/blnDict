<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

    <mapping map-null="false" map-empty-string="false" trim-strings="true">
    <class-a>kz.kegoc.bln.entity.dict.EnergySourceMeteringPoint</class-a>
    <class-b>kz.kegoc.bln.entity.dict.dto.EnergySourceMeteringPointDto</class-b>
    
     <field>
       <a>energySource.id</a>
       <b>energySourceId</b>
     </field>    

     <field>
       <a>meteringPoint.id</a>
       <b>meteringPointId</b>
     </field>

     <field type="one-way">
       <a>meteringPoint.name</a>
       <b>meteringPointName</b>
     </field>

     <field>
       <a>meteringType.id</a>
       <b>meteringTypeId</b>
     </field>

     <field type="one-way">
       <a>meteringType.name</a>
       <b>meteringTypeName</b>
     </field>
  </mapping> 
</mappings> 